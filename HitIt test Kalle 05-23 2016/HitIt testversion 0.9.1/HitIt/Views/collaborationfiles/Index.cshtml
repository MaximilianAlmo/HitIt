@model IEnumerable<HitIt.Models.collaborationfiles>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.collaborationFile)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.collaborations.ColName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.collaborationFile)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.collaborations.ColName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.collaborationFileId }) |
